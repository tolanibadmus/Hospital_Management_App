<div class="all-patients">
  <div class="title-wrapper">
    <p class="all-staff-p">All Staff</p>
    <a href="/staff/add" class="primary-button">Add New staff</a>
  </div>

  <table>
    <tbody>
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Gender</th>
        <th>Email Address</th>
        <th>Department</th>
        <th>Qualification</th>
        <th>Action</th>
      </tr>
      <% allStaff.forEach(staff => { %>
        <tr>
          <td><%= staff.firstName %></td>
          <td><%= staff.lastName %></td>
          <td><%= staff.gender %></td>
          <td><%= staff.emailAddress %></td>
          <td><%= staff.department %></td>
          <td><%= staff.qualification %></td>
          <td><a href="/staff/<%= staff._id %>" class="anchor-link">View more</a></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</div>