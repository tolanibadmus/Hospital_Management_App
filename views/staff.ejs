<div class="all-patients">
  <div class="title-wrapper">
    <p class="all-staff-p">All Staff</p>
    <a href="/staff/add" class="primary-button primary-button-anchor">Add New staff</a>
  </div>

  <table>
    <tbody>
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Gender</th>
        <th>Email Address</th>
        <th>Department</th>
        <th>Qualification</th>
        <th>Action</th>
      </tr>
      <% allStaff.forEach(staff => { %>
        <tr>
          <td><%= staff.firstName %></td>
          <td><%= staff.lastName %></td>
          <td><%= staff.gender %></td>
          <td><%= staff.emailAddress %></td>
          <td><%= staff.department %></td>
          <td><%= staff.qualification %></td>
          <td>
            <span class="anchor-link-wrapper">
              <a href="/staff/<%= staff._id %>/update" class="anchor-link">Edit</a>
              <form action ="/staff/<%= staff._id %>/delete" method="POST" class="anchor-link-form">
                <button class="anchor-link-delete" type="submit">Delete</button>
              </form>
            </span>
           
          </td>
          
          <!-- <form action ="/staff/<%= staff._id %>/update" method="POST" class="anchor-link-form"></form>
                <button class="anchor-link-edit" type="submit">Edit</button>
              </form> -->
          <!-- <td><a href="/staff/<%= staff._id %>" class="anchor-link">View more</a></td> -->
        </tr>
      <% }) %>
    </tbody>
  </table>
</div>